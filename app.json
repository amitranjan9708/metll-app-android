{
  "expo": {
    "name": "MetLL",
    "slug": "metll-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#A4B8E7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.metll.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#A4B8E7"
      },
      "package": "com.metll.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload your profile picture."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "The app uses your location to find people nearby for live matching."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e7fbd92f-1b9e-44da-bebd-0744b0f32acf"
      }
    }
  }
}
